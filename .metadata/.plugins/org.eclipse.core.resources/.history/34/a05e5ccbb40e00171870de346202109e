package textExcel;

public class ValueCell extends RealCell {
	
	public ValueCell(String value)
	{
		super(value);
	}
	public double getDoubleValue()
	{
		String ball=getCellText();
		
		double kkkkkk=Double.parseDouble(ball);
		return kkkkkk;
	}
	public String abbreviatedCellText(){
		String abbreviated=getCellText();		
		double kkkkkk=Double.parseDouble(abbreviated);
		abbreviated=kkkkkk+"";
		if(abbreviated.length() > 10){
			abbreviated+=(".0");
		}
		abbreviated+="               ";
		return abbreviated.substring(0,10);
	}
	public String fullTextCell(){
		return  getDoubleValue()+"";
	}
}
